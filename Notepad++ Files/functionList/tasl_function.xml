<?xml version="1.0" encoding="UTF-8" ?>
<NotepadPlus>
	<functionList>
		<parser
			displayName="TASL"
			id="tasl_function.xml"
		>
			<function
				mainExpr="#(?:\w|\.)+\s*(?:%[^%]*%)?\s*=\s*\{|\{#(?:\w|\.)+\s*(?:%[^%]*%)?"
			>
				<functionName>
					<nameExpr
						expr="#(?:\w|\.)+\s*(?:%[^%]*%)?"
					>
					</nameExpr>
				</functionName>
			</function>
		</parser>
	</functionList>
</NotepadPlus>